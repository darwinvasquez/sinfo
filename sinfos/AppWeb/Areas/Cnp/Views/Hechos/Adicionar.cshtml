    ViewBag.Title = "Adicionar";
}
@section scripts{
    <script>
        $(function () {
            //var urlPaises = "@Url.Action("ConsultarBarrio", "LugarGeografico")";

            $.ajax({
                type: 'POST',
                url: "@Url.Action("ConsultarBarrio", "LugarGeografico")",
                dataType: 'json',
                data: {},

                success: function (datos) {
                    $.each(datos, function (i, dato) {
                        $("#barrio").append('<option value="' + dato.Value + '">' + dato.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Fallo al traer los registros' + ex);
                }
            })

        });
    </script>
}

