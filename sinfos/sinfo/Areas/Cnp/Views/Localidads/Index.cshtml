            @Html.DisplayFor(modelItem => item.LocalidadId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoPonal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodigoMunicipio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Municipio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreLocalidad)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.LocalidadId }) |
