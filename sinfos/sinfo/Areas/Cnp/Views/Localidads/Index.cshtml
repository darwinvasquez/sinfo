@model IEnumerable<sinfo.Areas.Cnp.Models.Localidad>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<p>
    @Html.ActionLink("Sincroniza PONAL", "SincronizaLocalidad")
</p>
<table class="table">
    <tr>
